{
    "name": "ai-challenger-dashboard",
    "version": "0.0.1",
    "description": "AI Challenger interactive dashboard",
    "main": "js/app.js",
    "dependencies": {
        "babel-core": "~5.6.0",
        "uglify-js": "~2.4.0",
        "flux": "^2.0.0",
        "react": "^0.13.0",
        "browserify": "~10.2.0",
        "envify": "~3.0.0",
        "reactify": "^0.15",
        "watchify": "~2.1.0",
        "babelify": "~6.1.3"
    },
    "scripts": {
        "start": "watchify -o js/bundle.js -v -d .",
        "build": "browserify . | uglifyjs -cm > js/bundle.min.js"
    },
    "browserify": {
        "transform": [
          "babelify",
          "reactify",
          "envify"
        ]
    }
}
